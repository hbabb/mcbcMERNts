  "license": "ISC",
  "private": true,
  "type": "module",
  "main": "backend/index.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "directories": {
    "doc": "docs",
    "react": "frontend",
    "express": "backend",
    "docker": "docker",
    "test": "tests"
  },
  "scripts": {
    "security-check": "npm audit",
    "dev:backend": "nodemon backend/index.js",
    "dev:frontend": "vite",
    "postinstall": "[ ! \"$HUSKY_SKIP_INSTALL\" ] && node .husky/install.mjs || exit 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "docs": "npx typedoc --options typedoc.json",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint:markdown": "markdownlint docs/**/*.md project/**/*.md",
    "fix:markdown": "prettier --write docs/**/*.md project/**/*.md",
    "eslint": "eslint --report-unused-disable-directives --max-warnings=0 ./backend ./frontend",
    "eslint:fix": "eslint --report-unused-disable-directives --max-warnings=0 ./backend ./frontend --fix",
    "build:frontend": "tsc -b && vite build",
    "preview": "vite preview",
    "start:frontend": "serve -s dist",
    "clean": "rm -rf dist",
    "analyze": "vite build --mode analyze"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,less}": [
      "stylelint --fix"
    ],
    "*.md": [
      "markdownlint --fix"
    ],
    "*.{json,css,scss,md,less}": [
      "prettier --write"
    ]
  },
